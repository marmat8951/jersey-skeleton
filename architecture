PATH : localhost:8080/entreedetvous

 POST :
  
  /user/etudiant   Création d'un etudiant
  /user/senior     Création d'un senior

  {
   "nom":"nom",
   "prenom":"prenom",
   "telepone":"telephone",
   "login":"login",
   "mail":"mail",
   "mdp":"mdp"
   "planning" :
      ["horaire"{
       "jour":"jour",
       "moment":"moment"
    }]
   }
  }

  /service    Création d'un service

  { 
   "libelle":"libelle"
  }

  /rdv     Création d'un rdv
  
  {
   "senior":"senior",
   "etudiant":"etudiant",
   "jour":"jour",
   "moment":"moment",
   "service":"service",
   "description":"descriptio"
  }

 GET :
  
  /user/etudiant       Obtenir la liste des etudiants
  /user/senior         Obtenir la liste des seniors
  /user/etudiant/{id}  Obtenir l'etudiant suivant l'id
  /user/senior/{id}    Obtenir l'etudiant suivant l'id

  /user/etudiant?senior-id=id  Obtenir la liste des etudiants correspondants au senior

  /user/etudiant?hobby=1,2,..   Obtenir la liste des etudiants qui ont les hobbies passés en paramétres   

  /user/etudiant?jour=jour&moment=moment  Obtenir la liste des etudiants disponibles à cette plage horaire
  
  /service        Obtenir la liste des services
  /service/{id}   Obtenir le service suivant l'id
  /rdv            Obtenir la liste des rdv
  /rdv/{id}       Obtenir le rdv suivant l'id
  


 DELETE :
  
  /user/{id}  Suppression d'un utilisateur
  /service/{id}      Suppression d'un service
  /rdv/{id}          Suppression d'un rdv

 PUT :
 
  /user/etudiant/{id}  Mise a jour d'un etudiant
  /user/senior/{id}    Mise a jour d'un senior

  {
   "nom":"nom",
   "prenom":"prenom",    
   "telepone":"telephone",
   "login":"login",
   "mail":"mail",
   "mdp":"mdp"
  }

  /service/{id}   Mise a jour d'un service

  {
   "libelle":"libelle"
  }

  /rdv/{id}  Mise a jour d'un rdv

  {
   "senior":"senior",
   "etudiant":"etudiant",
   "jour":"jour",
   "moment":"moment",
   "service":"service",
   "description":"description",
   "fait":booleen
  }

  /rdv/{id}/done  Passe un rdv a Fait
  /rdv/{id}/undone   Passe  un rdv a non Fait
 
 
