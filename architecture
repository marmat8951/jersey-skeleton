cd PATH : localhost:8080/v1/

 POST :
  
  /user Création d'un utilisateur

{
"nom" : "{nom}",
"prenom" : "{prenom}",
"login" : "{login}",	(CLE PRIMAIRE)
"numero" : "{tnumero}",
"statut" : "{ETUDIANT|SENIOR}",
"email" : "{mail}",
"password" : "{password}"
}

  /service    Création d'un service

  { 
   "libelle":"libelle"
  }

  /rdv     Création d'un rdv
  
{
"senior" : "papy",
"jour" : "lundi",
"matin" : "true",
"aprem" : "true",
"soir" : "true",
"etudiant" : "Cyril",
"service" : "poney"
}

 GET :
  
  /user/etudiants       Obtenir la liste des etudiants
  /user/seniors         Obtenir la liste des seniors
  /user/valide		Obetnir la liste des utilisateurs validés
  /user/!valide		Obtenir la liste des utilisateurs non validés
  /user/{login}         Obtenir l'utilisateur selon le login

  /user/etudiant?senior-id=id  Obtenir la liste des etudiants correspondants au senior //TODO

  /user/etudiant?hobby=1,2,..   Obtenir la liste des etudiants qui ont les hobbies passés en paramétres //TODO  

  /user/etudiant?jour=jour&moment=moment  Obtenir la liste des etudiants disponibles à cette plage horaire //TODO
  
  /service        Obtenir la liste des services
  /service/{id}   Obtenir le service suivant le libelle 


 DELETE :
  
  /user/{id}  Suppression d'un utilisateur
  /service/{id}      Suppression d'un service
  /rdv/{id}          Suppression d'un rdv

 PUT :
 
  /user/{login} mise a jour d'un utilisateur
  /user/validate/{login} valide le compte d'utilisateur

{
  "nom" : "{nom}",
  "prenom" : "{prenom}",
  "numero" : "{tnumero}",
  "email" : "{mail}",
  "password" : "{password}"
}

  /service/{id}   Mise a jour d'un service

  {
   "libelle":"libelle"
  }


